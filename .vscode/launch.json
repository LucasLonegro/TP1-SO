    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug",
            "type": "gdb",
            "request": "launch",
            "target": "./bin/md5",
            "arguments": "${input:argv}",
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText"
        }
    ],
    "inputs": [
        {
            "id": "argv",
            "description": "File to test with (Only 1):",
            "default": "Makefile",
            "type": "promptString"
        },
    ],
}